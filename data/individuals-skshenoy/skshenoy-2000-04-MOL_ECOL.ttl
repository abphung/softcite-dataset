# https://howisonlab.github.io/softcite-dataset/pdf-files/2000-04-MOL_ECOL.pdf
bioj:a2000-04-MOL_ECOL rdf:type bioj:article ;

    citec:has_supplement [ rdf:type citec:supplement ;
                           citec:isPresent false ] ;

    citec:has_in_text_mention true ;

    citec:coded_no_in_text_mentions false ;
.
biojFIXME: rdf:type citec:in_text_mention ;
    citec:full_quote FIXME ;

    citec:on_pdf_page FIXME  ;

    citec:spans_pages FIXME ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "skshenoy" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label FIXME software | algorithm |  hardware | other;
                           ca:certainty FIXME1-10 ;
                           ca:memo FIXME ;
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "skshenoy" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent FIXME;
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "skshenoy" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent FIXME ;
                           rdfs:label FIXME ;
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "skshenoy" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent FIXME ;
                           rdfs:label FIXME ;
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "skshenoy" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent FIXME ;
                           rdfs:label FIXME ;
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "skshenoy" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent FIXME ;
                           rdfs:label FIXME ;
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "skshenoy" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent FIXME ;
                           rdfs:label FIXME ;
                         ] ;
        ] ;
    citec:has_reference bioj-citedFIXME: ;
.
