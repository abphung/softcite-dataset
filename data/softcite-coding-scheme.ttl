citec:has_selection
    rdf:type ca:ContentAnalysisCode ;
    rdfs:comment """FIXME""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:in-text_mention
    rdf:type ca:ContentAnalysisCode ;
    rdfs:comment """Mention of software in the text part of the paper""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:reference
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """A software relevant reference. If multiple references are present in the coding unit and refer to the same piece of software, multiple has_reference codes should be attached to the same in-text_mention, not broken up.""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:software_used
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """For mentions of software that was used in the research""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:software_not_used
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """For mentions of software that the authors didn't use (e.g., they discuss why they didn't use it).	This code should be used for software that the authors considered (e.g., the competitors of the software they used).""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:software_name
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """whatever the name is""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:version_number
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """whatever the version number is. If use date as version number include both codes""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:url
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """whatever the url is""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:date
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """A date used to indicate a version of the software.  Not the date of the paper or reference. If use date as version number include both codes""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:creator
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """A mention of the creator of the software, don't include names as citation anchors.  Haven't decided what to do about references here yet. Example of where to use: Software, written by xyz, was used for ... 'coupled to MetaMorph imaging software (Universal Imaging Corporation, Downingtown, PA).'  Code just the company name""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:memo
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """Any interesting issues that come up in the paper""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:configuration_details
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """Any interesting issues that come up in the paper""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:configuration_details
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """Any interesting issues that come up in the paper""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:publication
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """Reference to a paper primarily detailing the software itself (cited because it uses or introduces the software)""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:users_guide
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """Reference to a user’s manual for the software""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:project_page
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """URL for the software’s online presence - downloads, orders, FAQs""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:project_name
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """Descriptive data about the software""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.

citec:creator
    rdfs:type ca:ContentAnalysisCode ;
    rdfs:comment """unlike in the rest of the text, code the author names of the paper, and include the full string of multiple authors as a single value for a :creator code""" ;
    ca:codingOrder 1 ;
    ca:codingRound 1 ;
    ca:template """
                                           ca:comment \"\"""" ;
.
